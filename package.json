        "dotenv": "^8.2.0",
        "eslint": "^7.17.0",
        "eslint-config-prettier": "^7.1.0",
        "eslint-config-react-app": "^6.0.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "husky": "^4.3.6",
        "lint-staged": "^10.5.3",
        "prettier": "^2.2.1",
        "pretty-quick": "^3.1.0",
        "sonarqube-scanner": "^2.8.0",
        "sonarqube-verify": "^1.0.2"
    },
    "scripts": {
        "start-dev": "SKIP_PREFLIGHT_CHECK=true react-scripts start",
        "build": "NODE_ENV=production SKIP_PREFLIGHT_CHECK=true react-scripts build",
        "test": "CI=true SKIP_PREFLIGHT_CHECK=true react-scripts test --env=jsdom --passWithNoTests",
        "test-ci": "CI=true SKIP_PREFLIGHT_CHECK=true react-scripts test --env=jsdom --passWithNoTests",
        "test-watch": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "start": "NODE_ENV=production NODE_TLS_REJECT_UNAUTHORIZED=0 node server.js",
        "cf-push": "yarn --ignore-engines && npm run build && cf push",
        "lint": "eslint './src/**/*.{tsx,ts}'",
        "audit-dependencies": "audit-ci --config audit-ci.json",
        "lint-fix": "eslint './src/**/*.{tsx,ts}' --fix",
        "sonar": "sonarqube-verify"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "engines": {
        "node": "12.x.x"
    },
    "jestSonar": {
        "reportPath": "coverage",
        "reportFile": "test-reporter.xml",
        "indent": 4
    }
}